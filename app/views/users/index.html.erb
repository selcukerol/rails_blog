 <h2>BLOG USERS</h2>
 </br>           
 <table class="table table-hover">
 	<thead>
 		<tr>
 			<th>User Id</th>
 			<th>First</th>
 			<th>Last</th>
 			<th>DOB</th>
 			<th>Username</th>
 			<th>Action</th>
 			<th>Action</th>
 		</tr>
 	</thead>
 	<tbody>
 		<% @users.each do |user| %>
 		<tr>
 			<td><a href="/users/<%= user.id %>"><%= user.id %></a></td>
 			<td><%= user.fname %></td>
 			<td><%= user.lname %></td>
 			<td><%= user.dob.strftime "%D" if user.dob %></td>
 			<td><%= user.username %></td>
 			<td><%= link_to "Delete", user_path(user), method: :delete %></td>
 			<td><%= link_to "Edit", edit_user_path(user) %></td>
 		</tr>
 		<% end %>
 	</tbody>
 </table>







